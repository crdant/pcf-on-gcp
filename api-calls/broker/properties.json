{
  "properties": {
    ".properties.root_service_account_json": {
      "value": "${SERVICE_ACCOUNT_CREDENTIALS}"
    },
    ".properties.db_host": {
      "value": "${BROKER_DB_HOST}"
    },
    ".properties.db_username": {
      "value": "${BROKER_DB_USER}"
    },
    ".properties.db_password": {
      "value": {
        "secret": "${BROKER_DB_USER_PASSWORD}"
      }
    },
    ".properties.db_port": {
      "value": "3306"
    },
    ".properties.ca_cert": {
      "value": "${SERVER_CERT}"
    },
    ".properties.client_cert": {
      "value": "${CLIENT_CERT}"
    },
    ".properties.client_key": {
      "value": "${CLIENT_KEY}"
    }
  }
}
