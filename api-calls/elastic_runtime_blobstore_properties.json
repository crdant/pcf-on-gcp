{
  "properties": {
    ".properties.system_blobstore": {
      "type": "selector",
      "configurable": true,
      "credential": false,
      "value": "external_gcs",
      "optional": false
    },
    ".properties.system_blobstore.external_gcs.buildpacks_bucket": {
      "type": "string",
      "configurable": true,
      "credential": false,
      "value": "${BUILDPACKS_STORAGE_LOCATION}",
      "optional": false
    },
    ".properties.system_blobstore.external_gcs.droplets_bucket": {
      "type": "string",
      "configurable": true,
      "credential": false,
      "value": "${DROPLETS_STORAGE_LOCATION}",
      "optional": false
    },
    ".properties.system_blobstore.external_gcs.packages_bucket": {
      "type": "string",
      "configurable": true,
      "credential": false,
      "value": "${PACKAGES_STORAGE_LOCATION}",
      "optional": false
    },
    ".properties.system_blobstore.external_gcs.resources_bucket": {
      "type": "string",
      "configurable": true,
      "credential": false,
      "value": "${RESOURCES_STORAGE_LOCATION}",
      "optional": false
    },
    ".properties.system_blobstore.external_gcs.access_key": {
      "type": "string",
      "configurable": true,
      "credential": false,
      "value": "${GCP_ACCESS_KEY_ID}",
      "optional": false
    },
    ".properties.system_blobstore.external_gcs.secret_key": {
      "type": "secret",
      "configurable": true,
      "credential": true,
      "value": {
        "secret": "${GCP_SECRET_ACCESS_KEY}"
      },
      "optional": false
    }
  }
}
