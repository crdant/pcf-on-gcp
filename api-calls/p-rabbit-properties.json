{
  "properties": {
    ".rabbitmq-server.server_admin_credentials": {
      "type": "simple_credentials",
      "value": {
        "identity": "rabbitmq",
        "password": "${RABBIT_ADMIN_PASSWORD}"
      },
      "optional": false
    },
    ".rabbitmq-server.plugins": {
      "value": [
        "rabbitmq_management",
        "rabbitmq_mqtt",
        "rabbitmq_stomp",
        "rabbitmq_amqp1_0"
      ]
    }
  }
}
